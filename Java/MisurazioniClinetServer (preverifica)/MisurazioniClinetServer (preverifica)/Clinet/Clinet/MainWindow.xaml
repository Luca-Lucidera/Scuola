<Window x:Class="Clinet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Clinet"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="992">
    <Grid>
        <TextBox x:Name="txt_temperatura" HorizontalAlignment="Left" Margin="508,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="34"/>
        <TextBox x:Name="txt_umi" HorizontalAlignment="Left" Margin="508,138,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="34"/>
        <Button x:Name="btn_send" Content="SEND DATA" HorizontalAlignment="Center" Margin="0,223,0,0" VerticalAlignment="Top" Height="34" Width="255" Click="btn_send_Click"/>
        <Button x:Name="btn_analizza" Content="ANALIZZA DATI" HorizontalAlignment="Center" Margin="0,284,0,0" VerticalAlignment="Top" Height="34" Width="255" Click="btn_analizza_Click"/>
        <Label x:Name="label" Content="Temperatura" HorizontalAlignment="Left" Margin="336,82,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Umidita&#xD;&#xA;" HorizontalAlignment="Left" Margin="336,134,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
